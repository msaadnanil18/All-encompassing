{
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "quotes": ["error", "single"],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
      },
    ],
  },
  "env": {
    "browser": true,
    "es2021": true,
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
  },
  "settings": {
    "react": {
      "version": "detect",
    },
  },
}
